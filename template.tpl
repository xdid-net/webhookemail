___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const encodeUriComponent = require('encodeUriComponent');

const url = "https://xdid-balancer.net/gtm/webhookemail.js?param=" + encodeUriComponent(data.Parameter) + "&value=" + encodeUriComponent(data.Value) + "&emailparam=" + encodeUriComponent(data.EmailParameter) + "&url=" + encodeUriComponent(data.Webhook);

const onSuccess = () => {
  data.gtmOnSuccess();
};

const onFailure = () => {
  data.gtmOnFailure();
};

if (queryPermission('inject_script', url)) {
  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  data.gtmOnFailure();
}


